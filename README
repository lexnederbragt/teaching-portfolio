DEST=/Users/alexajo/github/teaching-portfolio/book
mkdir -p $DEST
doconce jupyterbook main.do.txt --show_titles --sep=section --dest=$DEST --dest_toc=$DEST --skip_inline_comments

jupyter-book build book

ghp-import -n -p -f docs/_build/html/

# references
doconce fix_bibtex4publish ~/Downloads/pedagogisk_mappe.bib
# next step may require manual intervention
cd src && publish import ~/Downloads/pedagogisk_mappe.bib
